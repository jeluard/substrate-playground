{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "generateName": "theia-substrate-http-",
        "labels": {
            "app": "theia-substrate",
            "instance-uuid": "%INSTANCE_UUID%"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "web",
                "protocol": "TCP",
                "port": 3000
            },
            {
                "name": "front-end",
                "protocol": "TCP",
                "port": 8000
            },
            {
                "name": "wss",
                "protocol": "TCP",
                "port": 9944
            }
        ],
        "selector": {
            "instance-uuid": "%INSTANCE_UUID%"
        },
        "type": "NodePort"
    }
}