{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "generateName": "theia-substrate-",
    "labels": {
      "app": "theia-substrate",
      "user-uuid": "%USER_UUID%",
      "instance-uuid": "%INSTANCE_UUID%"
    }},
  "spec": {
    "containers": [{
    "name": "theia-substrate-container",
    "image": "%IMAGE_NAME%"
    }]
  }
}