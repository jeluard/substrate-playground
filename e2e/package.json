{
  "name": "@parity/substrate-playground-e2e",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "ava -s --timeout=5m",
    "clean": "rm -rf dist/ node_modules/ yarn.lock"
  },
  "ava": {
    "files": [
      "test/**.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=esbuild-node-loader",
      "--experimental-specifier-resolution=node"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@substrate/playground-client": "1.9.18",
    "cross-fetch": "3.1.5",
    "esbuild-node-loader": "^0.8.0"
  },
  "devDependencies": {
    "@types/node": "18.6.0",
    "ava": "4.3.1",
    "playwright": "1.24.2",
    "typescript": "4.7.4"
  }
}
