      "--loader=@esbuild-kit/esm-loader"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@substrate/playground-client": "1.9.26",
    "cross-fetch": "3.1.5"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "2.5.0",
    "@types/node": "18.11.9",
    "ava": "5.0.1",
    "playwright": "1.27.1",
    "typescript": "4.8.4"
  }
}
