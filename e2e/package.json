      "--loader=ts-node/esm"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@substrate/playground-client": "1.8.4",
    "cross-fetch": "3.1.5",
    "yargs": "17.4.0"
  },
  "devDependencies": {
    "@types/node": "17.0.22",
    "@types/yargs": "17.0.10",
    "ava": "4.1.0",
    "playwright": "1.20.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  }
}
