{
  "name": "@parity/substrate-playground-e2e",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "ava -s --timeout=10m",
    "clean": "rm -rf dist/ node_modules/ yarn.lock"
  },
  "ava": {
    "files": [
      "test/**.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@substrate/playground-client": "1.9.0",
    "cross-fetch": "3.1.5"
  },
  "devDependencies": {
    "@types/node": "17.0.40",
    "ava": "4.3.0",
    "playwright": "1.22.2",
    "ts-node": "10.8.0",
    "typescript": "4.7.3"
  }
}
