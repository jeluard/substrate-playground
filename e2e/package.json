{
  "name": "@parity/substrate-playground-e2e",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "ava -s --timeout=5m",
    "clean": "rm -rf dist/ node_modules/ yarn.lock"
  },
  "ava": {
    "files": [
      "test/**.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=@esbuild-kit/esm-loader"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@substrate/playground-client": "1.9.23",
    "cross-fetch": "3.1.5"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "2.4.2",
    "@types/node": "18.7.18",
    "ava": "4.3.3",
    "playwright": "1.25.2",
    "typescript": "4.8.3"
  }
}
