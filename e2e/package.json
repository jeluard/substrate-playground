{
  "name": "@parity/substrate-playground-e2e",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "run:perf": "node dist/perf.js",
    "test": "ava",
    "clean": "rm -rf dist/ node_modules/ yarn.lock"
  },
  "ava": {
