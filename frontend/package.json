{
  "name": "@parity/substrate-playground-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "npx eslint ./src/ --ext .js,.jsx,.jsx,.ts,.tsx",
    "dev": "NODE_ENV=dev parcel public/index.html --port 81",
    "watch": "parcel watch public/index.html",
    "build": "parcel build public/index.html && mkdir -p dist/assets/ && cp -R public/assets/* dist/assets/ && cp public/robots.txt dist/",
    "start": "npm run build && cd dist/ && npx http-server",
    "clean": "rm -rf node_modules/ dist/ .parcel-cache/ package-lock.json"
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.4.1",
    "@mui/lab": "5.0.0-alpha.69",
    "@mui/material": "5.4.1",
    "@mui/styles": "5.4.2",
    "@substrate/playground-client": "1.7.2",
    "@xstate/react": "2.0.0",
    "marked": "4.0.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spring": "9.4.3",
    "xstate": "4.30.0"
  },
  "devDependencies": {
    "@mui/types": "7.1.2",
    "@parcel/transformer-inline-string": "2.3.1",
    "@types/marked": "4.0.2",
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "8.9.0",
    "eslint-plugin-react": "7.28.0",
    "parcel": "2.3.1",
    "postcss": "8.4.6",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "browserslist": [
    "last 5 and_chr versions",
    "last 5 chrome versions",
    "last 5 opera versions",
    "last 5 ios_saf versions",
    "last 5 safari versions"
  ]
}
