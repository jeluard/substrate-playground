{
  "name": "@parity/substrate-playground-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "npx eslint ./src/ --ext .js,.jsx,.jsx,.ts,.tsx",
    "dev": "NODE_ENV=dev parcel public/index.html --port 81",
    "watch": "parcel watch public/index.html",
    "build": "parcel build public/index.html && mkdir -p dist/assets/ && cp -R public/assets/* dist/assets/ && cp public/robots.txt dist/",
    "start": "yarn build && cd dist/ && npx http-server",
    "clean": "rm -rf node_modules/ dist/ .parcel-cache/ yarn.lock"
  },
  "dependencies": {
    "@emotion/react": "11.8.1",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.4.2",
    "@mui/lab": "5.0.0-alpha.70",
    "@mui/material": "5.4.3",
    "@mui/styles": "5.4.4",
    "@substrate/playground-client": "1.8.4",
    "@xstate/react": "2.0.0",
    "marked": "4.0.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spring": "9.4.3",
    "xstate": "4.30.2"
  },
  "devDependencies": {
    "@mui/types": "7.1.2",
    "@parcel/transformer-inline-string": "2.3.2",
    "@types/marked": "4.0.2",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.10.0",
    "eslint-plugin-react": "7.29.2",
    "parcel": "2.3.2",
    "typescript": "4.5.5"
  },
  "browserslist": [
    "last 5 and_chr versions",
    "last 5 chrome versions",
    "last 5 opera versions",
    "last 5 ios_saf versions",
    "last 5 safari versions"
  ]
}
