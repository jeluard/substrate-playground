{
  "name": "@parity/substrate-playground-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "npx eslint ./src/ --ext .js,.jsx,.jsx,.ts,.tsx",
    "dev": "NODE_ENV=dev parcel public/index.html --port 81 --https",
    "check": "tsc --noEmit",
    "watch": "parcel watch public/index.html",
    "build": "parcel build public/index.html && mkdir -p dist/assets/ && cp -R public/assets/* dist/assets/ && cp public/robots.txt dist/",
    "start": "yarn build && cd dist/ && npx http-server",
    "clean": "rm -rf node_modules/ dist/ .parcel-cache/ yarn.lock"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.6",
    "@mui/lab": "5.0.0-alpha.101",
    "@mui/material": "5.10.7",
    "@mui/styles": "5.10.8",
    "@substrate/playground-client": "1.9.24",
    "@xstate/react": "3.0.1",
    "marked": "4.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xstate": "4.33.6"
  },
  "devDependencies": {
    "@mui/types": "7.2.0",
    "@parcel/transformer-inline-string": "2.7.0",
    "@types/marked": "4.0.7",
    "@types/node": "18.8.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.24.0",
    "eslint-plugin-react": "7.31.8",
    "parcel": "2.7.0",
    "typescript": "4.8.4"
  },
  "browserslist": [
    "last 5 and_chr versions",
    "last 5 chrome versions",
    "last 5 opera versions",
    "last 5 ios_saf versions",
    "last 5 safari versions"
  ]
}
