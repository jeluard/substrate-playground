{
  "name": "@parity/substrate-playground-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "npx eslint ./src/ --ext .js,.jsx,.jsx,.ts,.tsx",
    "dev": "NODE_ENV=dev parcel public/index.html --port 81 --https",
    "watch": "parcel watch public/index.html",
    "build": "parcel build public/index.html && mkdir -p dist/assets/ && cp -R public/assets/* dist/assets/ && cp public/robots.txt dist/",
    "start": "yarn build && cd dist/ && npx http-server",
    "clean": "rm -rf node_modules/ dist/ .parcel-cache/ yarn.lock"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.6.2",
    "@mui/lab": "5.0.0-alpha.80",
    "@mui/material": "5.6.4",
    "@mui/styles": "5.6.2",
    "@substrate/playground-client": "1.8.7",
    "@xstate/react": "3.0.0",
    "marked": "4.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "xstate": "4.31.0"
  },
  "devDependencies": {
    "@mui/types": "7.1.3",
    "@types/marked": "4.0.3",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "eslint-plugin-react": "7.29.4",
    "parcel": "2.5.0",
    "typescript": "4.6.4"
  },
  "browserslist": [
    "last 5 and_chr versions",
    "last 5 chrome versions",
    "last 5 opera versions",
    "last 5 ios_saf versions",
    "last 5 safari versions"
  ]
}
